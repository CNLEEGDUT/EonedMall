<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.eoned.eonedmalll.eonedmall.MainActivity">

<FrameLayout
    android:id="@+id/fl_fragment"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintBottom_toTopOf="@id/btn_home"></FrameLayout>


    <com.eoned.eonedmalll.eonedmall.views.MainButtomBtn
        android:id="@+id/btn_home"
        android:layout_width="0dp"
        android:layout_height="@dimen/bom_btn_hei"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/btn_category"></com.eoned.eonedmalll.eonedmall.views.MainButtomBtn>

    <com.eoned.eonedmalll.eonedmall.views.MainButtomBtn
        android:id="@+id/btn_category"
        android:layout_width="0dp"
        android:layout_height="@dimen/bom_btn_hei"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/btn_home"
        app:layout_constraintRight_toLeftOf="@id/btn_cart"></com.eoned.eonedmalll.eonedmall.views.MainButtomBtn>

    <com.eoned.eonedmalll.eonedmall.views.MainButtomBtn
        android:id="@+id/btn_cart"
        android:layout_width="0dp"
        android:layout_height="@dimen/bom_btn_hei"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/btn_category"
        app:layout_constraintRight_toLeftOf="@id/btn_price"></com.eoned.eonedmalll.eonedmall.views.MainButtomBtn>

    <com.eoned.eonedmalll.eonedmall.views.MainButtomBtn
        android:id="@+id/btn_price"
        android:layout_width="0dp"
        android:layout_height="@dimen/bom_btn_hei"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/btn_cart"
        app:layout_constraintRight_toLeftOf="@id/btn_me"></com.eoned.eonedmalll.eonedmall.views.MainButtomBtn>

    <com.eoned.eonedmalll.eonedmall.views.MainButtomBtn
        android:id="@+id/btn_me"
        android:layout_width="0dp"
        android:layout_height="@dimen/bom_btn_hei"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/btn_price"
        app:layout_constraintRight_toRightOf="parent"></com.eoned.eonedmalll.eonedmall.views.MainButtomBtn>
</android.support.constraint.ConstraintLayout>
